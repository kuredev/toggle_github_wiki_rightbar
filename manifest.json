{
  "manifest_version":2,
  "name":"Toggle GitHub Wiki's RightBar",
  "version":"1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Toggle GitHub Wiki's RightBar"
  },
  "icons":{
    "128": "T128.png"
  },
  "permissions": [
    "https://github.com/*"
  ]
}
